{
  "name": "cometa-challenge",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Ricardo Q. Bazan",
    "email": "raquirozbazan@gmail.com",
    "url": "https://rcrd.space"
  },
  "scripts": {
    "build": "next build",
    "dev": "yarn react-env next dev",
    "start": "yarn react-env next start",
    "lint": "next lint",
    "react-env": "react-env --prefix NEXT_PUBLIC --",
    "storybook": "start-storybook -p 6006",
    "build:sb": "build-storybook -o ./public/storybook",
    "vercel-build": "run-s build:sb build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,ts,tsx}\"",
    "fix:format": "yarn prettier --write",
    "fix:lint": "next lint --fix",
    "check:lint": "next lint",
    "check:format": "yarn prettier --list-different",
    "check:types": "tsc --noEmit",
    "test:static": "run-p check:*",
    "test": "run-s test:*",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "@emotion/react": "^11.8.2",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "constate": "^3.3.0",
    "date-fns": "^2.28.0",
    "dinero.js": "^2.0.0-alpha.8",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "next": "12.1.0",
    "ohmyfetch": "^0.4.15",
    "path-to-regexp": "^6.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.28.0",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@hookform/devtools": "^4.0.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/lodash.groupby": "^4.6.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "babel-loader": "^8.2.3",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=14.x"
  }
}
